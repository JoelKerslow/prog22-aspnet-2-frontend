{
  "routes": [
    {
      "path": "/",
      "element": "Welcome"
    },
    {
      "path": "*",
      "element": "Welcome"
    },
    {
      "path": "/Onboarding",
      "element": "Onboarding"
    },
    {
      "path": "/Verification",
      "element": "VerificationCode"
    },
    {
      "path": "/SearchProducts",
      "element": "SearchProducts"
    },
    {
      "path": "/Signup",
      "element": "Signup"
    },
    {
      "path": "/Signin",
      "element": "Signin"
    },
    {
      "path": "/Profile",
      "element": "ProfilePage"
    },
    {
      "path":"/EditProfile",
      "element":"EditProfile"
    },
    {
      "path": "/ProductReview/:productId",
      "element": "ProductReviewForm"
    },
    {
      "path": "/OrderReview/:orderId",
      "element": "OrderReviewForm"
    },
    {
      "path": "/Products/:type/:value",
      "element": "Products"
    },
    {
      "path":"/Profile/PaymentMethods",
      "element":"PaymentMethod"
    },
    {
      "path": "/product/:productId",
      "element": "ProductDetails"
    }
  ],
  "fallback": {
    "element": "Welcome"
  }
}